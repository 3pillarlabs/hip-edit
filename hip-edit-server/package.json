{
  "name": "hip-edit-server",
  "version": "1.0.0",
  "description": "Hip Edit APIs running on a \"server\".",
  "main": "src/run.js",
  "scripts": {
    "babel-node": "babel-node src/run.js",
    "dev:start": "nodemon --exec npm run transpile",
    "prod": "node lib/run.js",
    "start": "npm run prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "flow": "flow",
    "transpile": "run-s flow babel-node",
    "flow:stop": "flow stop"
  },
  "author": "Sayantam Dey <sayantam@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3pillarlabs/hip-edit.git"
  },
  "bugs": {
    "url": "https://github.com/3pillarlabs/hip-edit/issues"
  },
  "homepage": "https://github.com/3pillarlabs/hip-edit#readme",
  "config": {
    "server": {
      "port": 9000
    },
    "logger": {
      "console": {
        "level": "debug"
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.18.2",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "flow-bin": "^0.68.0",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "express": "^4.16.2",
    "jasmine": "^3.1.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.1",
    "winston": "^2.4.1"
  }
}
